{
    "device": "gpu",
    "seed": 1,
    "data_path": "<path-to-data>",
    "bodies": ["A", "B", "C"],
    "dims": ["x", "y"],
    "data_frac": 1,
    "dtype": "float",

    "path_logs": "logs",
    "checkpoint_period": 100,

    "nn_type": "LA_SympNet",
    "modules": 10,
    "sublayer": 4,
    "activations": "sigmoid",

    "learning_rate": 0.001,
    "adam_beta1": 0.9,
    "adam_beta2": 0.999,

    "loss": "mse",
    "max_epochs": 25000,
    "batch_size": 200,

    "model_path": "."
}

{
    "device": "gpu",
    "seed": 1,
    "data_path": "<path-to-data>",
    "bodies": ["A", "B", "C"],
    "dims": ["x", "y"],
    "data_frac": 1,
    "dtype": "float",

    "path_logs": "logs",
    "checkpoint_period": 100,

    "nn_type": "MLP",
    "layer": 5,
    "neurons": 128,
    "activations": "sigmoid",

    "learning_rate": 0.001,
    "adam_beta1": 0.9,
    "adam_beta2": 0.999,

    "loss": "mse",
    "p_range": [-1.5, 1.5],
    "q_range": [-3, 3],
    "p_steps": 4,
    "q_steps": 4,
    "max_epochs": 25000,
    "batch_size": 200,

    "model_path": "."
}
